package codingTest.programmers;

public class Q12944 {
	public static void main(String[] args) {
		//평균 구하기
		int[] arr = new int[100];
		int sum = 0;
        int count = 0;
        
        for (int i=0; i<arr.length; i++) {
            sum += arr[i];
            count++;
        }
        System.out.println((double)sum/count);
	}
}
